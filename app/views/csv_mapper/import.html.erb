<h1><%= csv_mapper_t(:import_csv_file) %></h1>

<p><%= csv_mapper_t(:please_choose_a_csv_file) %>:</p>

<%= form_tag resource_url_proxy.url_for(action: 'import'), multipart: true do %>
  <div class="field">
    <%= file_field_tag 'file' %>
    <%= submit_tag csv_mapper_t(:import) %>
  </div>
<% end %>
